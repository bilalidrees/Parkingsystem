<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bilalidrees.parkingsystem">

    <application
        android:allowBackup="true"
        android:icon="@drawable/splash"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Welcome"
            android:label="@string/title_activity_welcome"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".parking_selection" />
        <activity android:name=".Signup" />
        <activity android:name=".user_Selection_view" />
        <activity android:name=".book_slot" />
        <activity android:name=".Cancel_Booking" />
        <activity android:name=".user_feedback" />
        <activity android:name=".Admin_Selection_View" />
        <activity android:name=".feedback_view_reply"></activity>
    </application>

</manifest>